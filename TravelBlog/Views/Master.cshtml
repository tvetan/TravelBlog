                                        <li id="menu-item-1645" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-1645"><a title="Level 3" href="https://colorlib.com/activello/level-3/">Level 3</a></li>
                                    </ul>
                                </li>
                                <li id="menu-item-1813" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-1813"><a title="Shop" href="https://colorlib.com/activello/shop/">Shop</a></li>
                            </ul>
                        </div>
                        <div class="nav-search">
                            <form action="https://colorlib.com/activello/" method="get"><input type="text" name="s" value="" placeholder="Search"><button type="submit" class="header-search-icon" name="submit" id="searchsubmit" value="Search"><i class="fa fa-search"></i></button></form>
                        </div>
                    </div>
                </div>
            </div>
        </nav><!-- .site-navigation -->


        <div class="container">
            <div id="logo">
                <h1 class="site-name">
                    <a class="navbar-brand" href="https://colorlib.com/activello/" title="Activello Theme Demo" rel="home"><img width="207" height="28" src="https://cdn.colorlib.com/activello/wp-content/uploads/sites/10/2015/11/logo.png" class="attachment-full size-full" alt="logo" scale="0">					</a>
                </h1><!-- end of .site-name -->

                <div class="tagline">Just another colorlib.com site</div>
            </div><!-- end of #logo -->

        </div>

    </header>

    @RenderBody()

    @*<footer class="field dark">
            <div class="container">
                <div class="row">
                    @{ Html.RenderPartial("BottomNavigation"); }
                </div>
            </div>
        </footer>*@

    <div id="footer-area">
        <footer id="colophon" class="site-footer" role="contentinfo">
            <div class="site-info container">
                <div class="row">
                    <div class="copyright col-md-12">
                        @{ Html.RenderPartial("BottomNavigation"); }
                    </div>
                </div>
            </div><!-- .site-info -->
            <div class="scroll-to-top" style="display: block;"><i class="fa fa-angle-up"></i></div><!-- .scroll-to-top -->
        </footer><!-- #colophon -->
    </div>

    <!-- Javascripts -->
    <script src="/js/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    @*<script src="/scripts/fanoe.js"></script>*@
</body>
</html>