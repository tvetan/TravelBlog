                        </a>
                    </div>
                    <div class="col-sm-12 col-md-8">
                        <nav>
                            @{ Html.RenderPartial("MainNavigation"); }
                        </nav>
                    </div>
                </div>
            </div>
            <div id="toggle" class="toggle">
                <a href="#" class="cross"><span></span></a>
            </div>
        </header>*@
